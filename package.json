{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "swagger": "docker run -p 80:8080 -e SWAGGER_JSON=/local/twitter-api-v2.json -v $(pwd)/docs/openapi:/local swaggerapi/swagger-ui",
    "generate:twitter-api": "docker run --rm -v \"${PWD}:/local\" openapitools/openapi-generator-cli generate -i /local/docs/openapi/twitter-api-v2.json -g typescript-axios -o /local/src/api-client/twitter-v2",
    "cfn:dynamodb:deploy": "aws cloudformation deploy --template-file ./template/dynamodb.yml --stack-name twitter-management-dynamodb",
    "deploy:lambda": "bash ./deployLambda.sh"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.337.0",
    "@aws-sdk/lib-dynamodb": "^3.338.0",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@tanstack/react-query": "^4.29.5",
    "@trpc/client": "^10.21.1",
    "@trpc/next": "^10.21.1",
    "@trpc/react-query": "^10.21.1",
    "@trpc/server": "^10.21.1",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "axios": "^1.3.5",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "iron-session": "^6.3.1",
    "next": "13.2.4",
    "openid-client": "^5.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "^17.4.0",
    "recoil": "^0.7.7",
    "superjson": "^1.12.3",
    "typescript": "5.0.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "esbuild": "^0.17.19"
  }
}
